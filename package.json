{
   "name": "@silver886/TypeScript",
   "version": "0.0.0",
   "description": "TypeScript template",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "scripts": {
      "comments": "leasot --ignore .git/**,node_modules/** --tags NOTE OPTIMIZE TODO HACK XXX FIXME BUG --filetype .ts **",
      "format": "prettier --write . && eslint --fix .",
      "format-check": "prettier --check . && eslint .",
      "watch-test": "npm run test -- --watchAll",
      "test": "jest --coverage",
      "predev-start": "npm run tsoa-gen",
      "dev-start": "env NODE_ENV=local ts-node src/index.ts",
      "start": "node dist/index.js",
      "prebuild": "npm run tsoa-gen",
      "build": "tsc",
      "prebundle": "npm run tsoa-gen",
      "bundle": "ts-node --compilerOptions={\\\"module\\\":\\\"ESNext\\\"} --esm esbuild.config.ts",
      "prepublish": "npm run build",
      "prepublishOnly": "npm run test",
      "tsoa-gen": "tsoa spec-and-routes"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/silver886/TypeScript.git"
   },
   "keywords": [],
   "author": {
      "name": "Leo Liu",
      "email": "karta1041337@gmail.com"
   },
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/silver886/TypeScript/issues"
   },
   "homepage": "https://github.com/silver886/TypeScript#readme",
   "devDependencies": {
      "@silver886/eslint-config": "^2.3.0",
      "@tsoa/cli": "^5.1.1",
      "@types/body-parser": "^1.19.3",
      "@types/compression": "^1.7.3",
      "@types/cookie-parser": "^1.4.4",
      "@types/cors": "^2.8.14",
      "@types/express": "^4.17.18",
      "@types/jest": "^29.5.5",
      "@types/node": "^18.18.0",
      "@types/swagger-ui-dist": "^3.30.2",
      "@types/swagger-ui-express": "^4.1.4",
      "@types/yargs": "^17.0.26",
      "@typescript-eslint/eslint-plugin": "^6.7.3",
      "@typescript-eslint/parser": "^6.7.3",
      "esbuild": "^0.19.4",
      "eslint": "^8.50.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-import-resolver-node": "^0.3.9",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-prettier": "^5.0.0",
      "jest": "^29.7.0",
      "jest-junit": "^16.0.0",
      "leasot": "^13.3.0",
      "prettier": "^3.0.3",
      "prettier-plugin-multiline-arrays": "^2.0.0",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.2.2",
      "yargs": "^17.7.2"
   },
   "dependencies": {
      "@paralleldrive/cuid2": "^2.2.2",
      "@silver886/error-context": "^0.0.2",
      "@tsoa/runtime": "^5.0.0",
      "@vendia/serverless-express": "^4.10.4",
      "body-parser": "^1.20.2",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "helmet": "^7.0.0",
      "http-status-codes": "^2.3.0",
      "swagger-ui-dist": "^5.7.2",
      "swagger-ui-express": "^5.0.0"
   }
